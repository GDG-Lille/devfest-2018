{
  "entrypoint": "index.html",
  "shell": "src/df-app.html",
  "fragments": [
    "src/home/df-home.html",
    "src/home/df-intro.html",
    "src/home/df-map.html",
    "src/home/df-place.html",
    "src/home/df-sponsors.html",
    "src/faq/df-faq.html",
    "src/faq/df-code-of-conduct.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "public/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
